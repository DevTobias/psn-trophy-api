{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.14",
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.6",
    "@nestjs/core": "^8.4.6",
    "@nestjs/graphql": "^10.0.15",
    "@nestjs/platform-fastify": "^8.4.6",
    "@nestjs/typeorm": "^8.1.2",
    "apollo-server-fastify": "^3.8.2",
    "axios": "^0.27.2",
    "cache-manager": "^4.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-fetch": "^3.1.5",
    "fastify-cookie": "^5.7.0",
    "fastify-csrf": "^3.1.0",
    "fastify-helmet": "^7.1.0",
    "graphql": "^16.5.0",
    "graphql-query-complexity": "^0.11.0",
    "graphql-subscriptions": "^2.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "node-fetch": "^3.2.5",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@infotition/eslint-config": "^2.2.0",
    "@infotition/prettier-config": "^1.0.0",
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.6",
    "@types/cache-manager": "^3.4.3",
    "@types/node": "^17.0.40",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "prettier": "@infotition/prettier-config",
  "eslintConfig": {
    "extends": [
      "@infotition/eslint-config"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
